{
   "include": ["src", "types"],
   "compilerOptions": {
      "module": "esnext",
      "target": "esnext",
      "outDir": "dist",
      "lib": ["dom", "esnext"],
      "importHelpers": true,
      "declaration": true,
      "sourceMap": true,
      "rootDir": "./src",
      "strict": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "jsx": "react",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noEmit": true
   }
}